<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd" version="2.2">

    <!-- ================== REGRAS DE NAVEGACAO ================== -->
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>agendaList</from-outcome>
            <to-view-id>/app/agenda/list.jsf</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>agendaEdit</from-outcome>
            <to-view-id>/app/agenda/edit.jsf</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
</faces-config>
